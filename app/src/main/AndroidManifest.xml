<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.store.okidosmobileapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateCartListActivity"></activity>
        <activity android:name=".ShoppingCartActivity" />
        <activity android:name=".ProductDetailsActivity" />
        <activity android:name=".InsertProduct" />
        <activity
            android:name=".HomeFragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CategoriesFragment" />
        <activity android:name=".MenCatagoriesFragment" />
        <activity android:name=".WomenCatogoriesFragment" />
        <activity android:name=".MyAccountFragment" />
        <activity android:name=".AppSettingsFragment" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>